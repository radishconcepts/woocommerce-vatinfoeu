{
	"name": "radishconcepts/woocommerce-vatinfoeu",
	"description": "Integrates the EU VAT API with your WooCommerce powered website",
	"type": "wordpress-plugin",
	"support"    : {
		"issues": "https://github.com/radishconcepts/woocommerce-euvatinfo/issues",
		"source": "https://github.com/radishconcepts/woocommerce-euvatinfo"
	},
	"require": {
		"xrstf/composer-php52": "1.*",
		"composer/installers": "~1.0"
	},
	"scripts": {
		"post-install-cmd": [
			"xrstf\\Composer52\\Generator::onPostInstallCmd"
		],
		"post-update-cmd": [
			"xrstf\\Composer52\\Generator::onPostInstallCmd"
		],
		"post-autoload-dump": [
			"xrstf\\Composer52\\Generator::onPostInstallCmd"
		]
	},
	"autoload": {
		"classmap": [
			"includes/abstracts",
			"includes/admin",
			"includes/core",
			"includes/repositories"
		]
	}
}
